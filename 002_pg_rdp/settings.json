{
    "name": "rdp",

    "js_files": ["js/panel-lex.js"],

    "panels": {
        "lex": {
            "use": true,
            "isCustom": true,
            "containerId": "lexContainer",
            "name": "Lex",
            "varName": "lex",
            "conf": "{ container: lexContainer }"
        },

        "buttons": {
            "use": true,
            "name": "Buttons",
            "varName": "buttons",
            "conf": "{ container: buttonsContainer, showRunButton: true }"
        },

        "varsTable": {
            "use": true,
            "name": "VarsTable",
            "varName": "varsTable",
            "conf": "{ container: varsTableContainer, minNumRows: 6 }"
        },

        "console": {
            "use": false,
            "name": "Console",
            "varName": "console",
            "conf": "{ container: consoleContainer }"
        },

        "asmTable": {
            "use": true,
            "name": "AsmTable",
            "varName": "asmTable",
            "conf": "{ container: asmTableContainer, html: html }"
        },

        "tree": {
            "use": false,
            "name": "Tree",
            "varName": "tree",
            "conf": "{ container: treeContainer, width: 960, height: 300 }"
        },

        "callTree": {
            "use": true,
            "name": "CallTree",
            "varName": "callTree",
            "conf": "{ container: callTreeContainer, width: 960, height: 300, userNodeChar: true }"
        }
    }
}
